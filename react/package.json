    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "immer": "^11.1.3",
    "js-cookie": "^3.0.5",
    "react": "^19.2.3",
    "react-router": "^7.12.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "use-immer": "^0.11.0",
    "vaul": "^1.1.2",
    "zod": "^4.3.5",
    "zustand": "^5.0.10"
