    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/ui": "4.0.14",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint-plugin-better-tailwindcss": "^4.0.0",
    "happy-dom": "^20.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "oxfmt": "^0.24.0",
    "oxlint": "^1.39.0",
    "oxlint-tsgolint": "^0.11.0",
    "react-compiler-runtime": "^1.0.0",
    "react-dom": "^19.2.3",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "unplugin-icons": "^22.5.0",
    "vite": "^8.0.0-beta.4",
    "vitest": "^4.0.16"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "oxlint",
      "oxfmt"
    ]
  },
  "paths": {
    "@/*": "./src/*",
    "@/components/*": "./src/components/*"
  }
